{{> head}}
	<body>
		<h1>{{title}}</h1>
		{{#if markdown}}
		{{{markdown}}}
		{{/if}}

		{{#if content}}
		{{{content}}}
		{{/if}}

		{{#isEqual env 'dev'}}
		<script type="text/javascript" data-main="js/config.js" src="../components/requirejs/require.js"></script>
		<script src="http://localhost:{{config.livereload}}/livereload.js"></script>
		{{else}}
		<script type="text/javascript" src="app.js"></script>
		{{/isEqual}}
	</body>
</html>